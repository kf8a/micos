<div class="">

  <%= if @sampling do %>
  <button phx-click="stop">Stop sample</button>
  <% else %>
  <button phx-click="sample">Start sample</button>
  <% end %>

  <div>
    <%= @datetime%>
  </div>

  <h2>Data</h2>
  <div>
    <ul>
    <%= for key <- @data do %>
    <li><%= inspect(key) %></li>
  <% end %>
    </ul>
</div>
